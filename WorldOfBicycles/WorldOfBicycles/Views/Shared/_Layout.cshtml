    <header id="header" class="fixed-top">
        <nav class="navbar navbar-expand-lg navbar-dark" style="background-color: black;">

            @if (User.IsInRole("admin"))
            {
                <input type="checkbox" id="check">
                <label for="check">
                    <i class="fas fa-bars" id="btn"></i>
                    <i class="fas fa-times" id="cancel"></i>
                </label>

                <div class="sidebar">
                    <header>Панель админа</header>
                    <ul style="margin-top:20px;">
                        <li><a asp-controller="AddToDataBase" asp-action="Index"><i class="fas fa-qrcode"></i>Добавить товар</a></li>
                        <li><a asp-controller="AddToDataBase" asp-action="Categories"><i class="fas fa-link"></i>Добавить категории</a></li>
                        <li><a asp-controller="RemoveProduct" asp-action="Index"><i class="fas fa-link"></i>Удалить товар</a></li>
                        <li><a asp-controller="Users" asp-action="Index"><i class="fas fa-sliders-h"></i>Управление пользователями</a></li>
                        <li><a asp-controller="Roles" asp-action="Index"><i class="fas fa-sliders-h"></i>Управление ролями</a></li>

                        @*<li><a href="#"><i class="fas fa-calendar-week"></i>Events</a></li>
        <li><a href="#"><i class="far fa-question-circle"></i>About</a></li>
        <li><a href="#"><i class="fas fa-stream"></i>Services</a></li>
        <li><a href="#"><i class="far fa-envelope"></i>Contact</a></li>*@
                    </ul>
                </div>
            }

            <div class="container">
                <span class="navbar-brand">Велопрокат</span>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navContent"
                        aria-controls="navContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navContent">
                    <ul class="navbar-nav mr-auto mb-3 mb-lg-0">
                        <li class="nav-item">
                            <a asp-controller="Home" asp-action="Index" class="nav-link">Home</a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Shop" asp-action="Index" class="nav-link">Shop</a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Chat" asp-action="Index" class="nav-link">Chat</a>
                        </li>
                        
                    </ul>
                    <ul class="navbar-nav nav-flex-icons">
                        <li class="nav-item">
                            <a asp-controller="ShopCart" asp-action="Index" class="nav-link waves-effect">
                                <span style="background-color: #ffc107; color:black;" class="my-badge z-depth-1 mr-1">@ViewBag.ProductCount</span>
                                <i class="fa fa-shopping-cart"></i>
                                <span class="clearfix d-none d-sm-inline-block">Cart</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link waves-effect"></a>
                        </li>

                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link waves-effect">@User.Identity.Name</a>
                            </li>

                            <li class="nav-item">
                                <a asp-controller="Account" asp-action="Logout" class="nav-link waves-effect">Выход</a>
                            </li>
                        }
                        else
                        {
                            <li class="nav-item">
                                <a asp-controller="Account" asp-action="Login" class="nav-link waves-effect">Войти</a>
                            </li>

                            <li class="nav-item">
                                <a asp-controller="Account" asp-action="Register" class="nav-link waves-effect">Регистрация</a>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </nav>
    </header>


